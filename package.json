{
  "name": "ts-nextjs-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "npm run migrate && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && pnpm format",
    "lint:strict": "eslint --max-warnings=0 src",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prepare": "husky install",
    "testSignatureHelper": "npx ts-node ./src/lib/signature/signatureHelper.ts",
    "knex": "knex",
    "migrate": "npx knex migrate:unlock --knexfile ./src/database/knexfile.js && npx knex migrate:latest --knexfile ./src/database/knexfile.js",
    "migrate:down": "npx knex migrate:down --knexfile ./src/database/knexfile.js"
  },
  "dependencies": {
    "@sentry/node": "^8.3.0",
    "@simplewebauthn/browser": "^10.0.0",
    "@simplewebauthn/server": "^10.0.0",
    "@simplewebauthn/types": "^10.0.0",
    "@toruslabs/fetch-node-details": "^13.4.0",
    "@toruslabs/http-helpers": "^6.1.1",
    "@toruslabs/openlogin-utils": "^8.1.2",
    "@toruslabs/torus.js": "^12.3.5",
    "@types/uuid": "^9.0.8",
    "@web3auth/base": "^8.6.1",
    "@web3auth/ethereum-provider": "^8.6.1",
    "@web3auth/single-factor-auth": "8.0.0-beta.1",
    "base64url": "^3.0.1",
    "clsx": "^2.0.0",
    "ethereum-cryptography": "^2.1.3",
    "hono": "^4.3.10",
    "ioredis": "^5.4.1",
    "jose": "^5.3.0",
    "knex": "^3.1.0",
    "knex-tiny-logger": "^2.1.0",
    "loglevel": "^1.9.1",
    "lucide-react": "^0.376.0",
    "mysql2": "^3.9.7",
    "next": "^14.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "redis": "^4.6.14",
    "tailwind-merge": "^2.1.0",
    "uuid": "^9.0.1",
    "web3": "^4.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.3.0",
    "@commitlint/config-conventional": "^16.2.4",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@types/react": "^18.2.45",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.9.0",
    "next-sitemap": "^2.5.28",
    "postcss": "^8.4.32",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.3.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=60",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  }
}
